<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@android:color/white"
    tools:context="com.softgarden.garden.view.YingYeYuan.YYYActivity">

    <com.jeremyfeinstein.slidingmenu.lib.SlidingMenu
        xmlns:sliding="http://schemas.android.com/apk/res-auto"
        android:id="@+id/slidingmenulayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        sliding:behindWidth="220dp"
        sliding:behindScrollScale="1"
        sliding:fadeDegree="0.7"
        sliding:fadeEnabled="true"
        sliding:shadowDrawable="@drawable/shadow"
        sliding:shadowWidth="15dp"
        sliding:touchModeAbove="margin"
        sliding:viewBehind="@layout/layout_menu"
        sliding:viewAbove="@layout/layout_yyy_main" />
</LinearLayout>
